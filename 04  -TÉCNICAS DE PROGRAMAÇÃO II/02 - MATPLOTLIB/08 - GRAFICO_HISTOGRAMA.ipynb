    "import matplotlib.pyplot as plt\n",
    "import random\n",
    "import numpy as np\n",
    "\n",
    "x = np.random.rand(100) # Cria 100 números aleatórios\n",
    "\n",
    "plt.hist(x)\n",
    "\n",
    "plt.show()\n"

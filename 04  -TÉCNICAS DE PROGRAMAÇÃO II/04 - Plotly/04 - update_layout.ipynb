    "import plotly.express as px\n",
    "x = [1, 2, 3, 4, 5 ,6]\n",
    "y = [10, 5, 3, 4, 6, 8]\n",
    "z = ['a', 'b', 'c', 'c', 'a', 'a']\n",
    "\n",
    "fig = px.scatter(x=x, y=y, color=z)\n",
    "\n",
    "# Altera template do gr√°fico\n",
    "fig.update_layout(template='presentation')"

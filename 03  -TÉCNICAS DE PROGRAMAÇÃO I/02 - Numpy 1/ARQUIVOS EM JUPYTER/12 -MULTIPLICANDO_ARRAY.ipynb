    "import numpy as np\n",
    "import random\n",
    "from random import randint \n",
    "\n",
    "arr = np.random.randint(0, 100, 10)\n",
    "n = 20\n",
    "# map(<função>, <iteravel>) -> iteravel [lista, tupla, ...]\n",
    "lista_20 = list(map(lambda x: x*n, arr))\n",
    "print(lista_20)"
   ]
